{
  "name": "robertbakker/big-calculator",
  "type": "library",
  "description": "Calculator with big numbers",
  "require": {
    "php": "^7.1",
    "brick/math": "0.7.* | 0.8.*",
    "nette/tokenizer": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "spatie/phpunit-watcher": "^1.6",
    "phpbench/phpbench": "@dev",
    "larapack/dd": "1.*",
    "codedungeon/phpunit-result-printer": "^0.23.4"
  },
  "autoload": {
    "psr-4": {
      "RobertBakker\\BigCalculator\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Bakker",
      "email": "r.m.bakker@live.nl"
    }
  ],
  "scripts": {
    "test": "./vendor/bin/phpunit-watcher watch",
    "bench": "./vendor/bin/phpbench run --report=default"
  },
  "config": {
    "process-timeout": 0
  }
}
